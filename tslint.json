{
  "rules": {
    "indent": [ true, "spaces" ],
    "quotemark": [ false, "avoid-escape" ],
    "trailing-comma": [
      true,
      {
        "multiline": {
          "objects": "always",
          "arrays": "always",
          "functions": "never",
          "typeLiterals": "ignore"
        }
      }
    ],
    "no-var-requires": false,
    "ordered-imports": false,
    "member-ordering": [false],
    "object-literal-sort-keys": false,
    "no-shadowed-variable": false,
    "no-console": [false],
    "semicolon": [true, "never"]
  }
}
